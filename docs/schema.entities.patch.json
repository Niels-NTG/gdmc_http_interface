				"data": {
					"type": "object",
                    "title": "Entity Data",
                    "description": "Object containing entity data (https://minecraft.fandom.com/wiki/Entity_format#Entity_Format) information",
                    "examples": [
                        {
                            "variant": "minecraft:red",
                            "HasVisualFire": true,
                            "Invulnerable": true
                        }
                    ]
				}
			},
			"required": [
				"data",
				"uuid"
			],
			"title": "Entity Patch Instruction Element"
		}
	}
}
